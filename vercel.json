{
  "functions": {
    "api/index.py": {
      "runtime": "[email protected]"
    },
    "api/debug.py": {
      "runtime": "[email protected]"
    }
  },
  "routes": [
    {
      "src": "/api/debug",
      "dest": "/api/debug.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}