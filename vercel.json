{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/debug.py", 
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/debug",
      "dest": "/api/debug.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}